<script language=javascript>
 function init(){
    var ua = navigator.userAgent;
    var msieOffset = ua.indexOf("MSIE ");
	if(msieOffset>=0){
	  var msieVersion = parseFloat(ua.substring(msieOffset + 5, ua.indexOf(";", msieOffset)));
	  if(msieVersion < 6){
	     alert("请将您的IE升级到6.0或以上，否则会无法正常使用系统。");
	  }
	}	
	window.location.replace("/SignOnServlet");
 } 
 init();
