<script>
    	/**
	 * 校验密码强度
	 */
    $(document).ready(function(){
		var ispass = document.cookie;
		if (ispass.indexOf("ispass=true") > 0)
		 {
			alert("您的登陆密码太过简单，存在安全风险，请点击【确定】前往修改密码！");
			window.location.href = "http://ics.chinasoftosg.com:8001/sso/login?targetUrl=person/password/update&targetUrlType=2&from=portals";
		 }
	});
    var oa_domain = "http://ics.chinasoftosg.com:8888/";
	var oa2_domain = "http://ics.chinasoftosg.com:8888/";
	var new_oa_domain="http://oa.chinasoftosg.com:8888/";
    var oabak_domain = "http://183.239.143.212/";
    var oa_history = "http://oa.chinasoftosg.com:8081/";
	var it_domain = "http://its.chinasoftosg.com/";
	//var opt_domain = "http://op.chinasoftosg.com/";
    var opt_domain = " http://op.chinasoftosg.com:8080/";
	var attendance_domain = "http://kq.chinasoftosg.com/";
	var payment_domain = "http://sse.chinasoftosg.com/SSE/sso/login.do";
	//var ics_domain = "http://ics.chinasoftosg.com/";
	var ics_domain = "http://ics.chinasoftosg.com/";
	var EM_domain = "http://rm.chinasoftosg.com/";


	
	function gopay(path){
		var action = $("#router-form").attr("action");
		$("#router-form").attr("action",payment_domain+path);
		$("#router-form").attr("method","post");
		document.getElementById("router-form").submit();
		$("#router-form").attr("action",action);
		$("#router-form").attr("method","get");
	}
	function goattendance(path, target){
		var action = $("#router-form").attr("action");
		$("#router-form").attr("action",attendance_domain+path);
		$("#router-form").attr("method","post");
		$("#router-form").attr("target",target);
		document.getElementById("router-form").submit();
		$("#router-form").attr("action",action);
		$("#router-form").attr("method","get");
	}
	function goOpt(path, target){
		var action = $("#router-form").attr("action");
		$("#router-form").attr("action",opt_domain+path);
		$("#router-form").attr("method","post");
		$("#router-form").attr("target",target);
		document.getElementById("router-form").submit();
		$("#router-form").attr("action",action);
		$("#router-form").attr("method","get");
	}
	function goit(path, target){
		var action = $("#router-form").attr("action");
		if(path!=''){
			window.open(it_domain+"/router.php"+path);
			return;
		}
		$("#router-form").attr("action",it_domain+"/router.php"+path);
		$("#router-form").attr("method","post");
		$("#router-form").attr("target",target);
		document.getElementById("router-form").submit();
		$("#router-form").attr("action",action);
		$("#router-form").attr("method","get");
	}
	function goOA(linkpage,target){
		linkpage=linkpage?linkpage:"";
		document.getElementById("linkpage").value=linkpage;
		var f=document.getElementById("router-form");
		var action1= f.action;
		f.action = oa_domain + "router.php";
		if(target){
			f.target=target;
		}
		f.submit();
		f.action=action1;
	}


	function goOA2(linkpage,target){
