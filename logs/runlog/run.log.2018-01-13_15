<script>
	var erroMsg = document.getElementById("errorfield");
	var erroBox = document.getElementById("errorBox");

	if(erroMsg != null){
		$(erroBox).text($(erroMsg).text());
		erroBox.style.display = "block";
		setTimeout(function(){
			erroBox.style.display = "none";
		},10000);
	};
	var animateBrowserTip = function(){
	
		$('#browser_tip').animate({
			height:'+=100px',
		},'slow');
	};
</script>


</body>
