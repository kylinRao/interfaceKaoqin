		document.getElementById("linkpage").value=linkpage;
		var f=document.getElementById("router-form");
		var action1= f.action;
		f.action=new_oa_domain+"sso.route?target=c3lzdGVtL2ZyYW1lLzQvaW5kZXguanNw";
		if(target){
			f.target=target;
		}
		f.submit();
		f.action=action1;
	}


